{
  "tabWidth": 2,
  "useTabs": false,
  "semi": false,
  "printWidth": 120,
  "singleQuote": true,
  "trailingComma": "all",
  "plugins": ["@trivago/prettier-plugin-sort-imports"],
  "importOrder": [
    "^(@(?!components|modules|utils|context|hooks|pages|assets|i18n|styles|typings)|[a-z])",
    "^@(.*)$",
    "^[(.*)/]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
